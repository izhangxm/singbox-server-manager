# 用来生成server配置文件的配置文件
# ---------------
# 写入目标config的路径
dst_server_cfg: /etc/sing-box/config.json

# 订阅文件模板
subscribe_clash_tp: ./data/clash_tp.yaml

subscribe_clashmeta_tp: ./data/clashmeta_tp.yaml

subscribe_singbox_tp: ./data/singbox_tp.yaml


# 远程调用的rpc密钥
rpc_key: "123456"

# 国家和url地址
contry: us
server_url: banw01.duorenhezugia.top


# 用户相关配置
users:
  xiaohei:
    nickname: 王小黑
    auth: '123456'
    password: IM1ulkYXo5jcajeB//RMkA==
  dabao:
    nickname: 李大宝
    auth: '123456'
    password: axYqixX6cJQH1RuPuhQrGQ==

# dns相关配置
dns:
  strategy: ipv4_only
  disable_cache: false
  disable_expire: false

log:
  disabled: false
  level: debug
  # output: /tmp/sing-box.log
  timestamp: true

# 入站相关配置
inbounds:
  # 涉及到shadowtls的配置
  shadowtls:
    # 公共参数，其他版本upate这个配置
    common:
      type: shadowtls
      handshake:
        server: m.media-amazon.com
        server_port: 443
    # v2的特殊扩展
    v2:
      password: D4sq9uySspZ+bvMP+Ew0Ww==
  
  # tls的公共配置
  tls:
    enabled: true
    alpn:
      - h2
      - http/1.1
    min_version: "1.2"
    max_version: "1.3"
    cipher_suites: TLS_AES_128_GCM_SHA256
    acme:
      domain: banw01.duorenhezugia.top
      email: izhangxm@outlook.com
      provider: letsencrypt
    reality:
      enabled: false
      
  transport:
    type: ws
    path: /transport/ws
    early_data_header_name: Sec-WebSocket-Protocol 


  # 监听的公共配置，其他入站协议根据自己特点update
  listen:
    tcp_fast_open: true
    listen: '::'
  # 入站的协议，名字为tag
  interfaces:
    shadowsocks-in:
      meta: # 
        # only_tls:只生成通过stls的服务 only_me:仅生成自身的服务 all: 全部
        shadowtls_mode: all
        tls_s_port: 8443 # 如果overshadowtls为true，这一项必填
        support_multiuser: true # 是否支持多用户模式
        server_s_port: 0 # 如果不支持，此项必填，需要为每个用户开一个服务端口，注意不能与其他地方冲突
        uname_key: name
        use_tls: false
        use_transport: false
      content: # 基本模板
        type: shadowsocks
        listen_port: 20231
        method: 2022-blake3-aes-128-gcm
        password: eeHTjVJuVBjFbtx7tvIQIA==
    # http-in:
    #   meta:
    #     shadowtls_mode: only_tls
    #     tls_s_port: 8453
    #     support_multiuser: true
    #     server_s_port: 0
    #     uname_key: username
    #     use_tls: false
    #     use_transport: false
    #   content:
    #     type: http
    #     listen_port: 20232
    trojan-in:
      meta: # 
        shadowtls_mode: all
        tls_s_port: 8463 # 如果overshadowtls为true，这一项必填
        support_multiuser: true # 是否支持多用户模式
        server_s_port: 0 # 如果不支持，此项必填，需要为每个用户开一个服务端口，注意不能与其他地方冲突
        uname_key: name
        use_tls: true
        use_transport: true
      content: # 基本模板
        type: trojan
        listen_port: 20233
        transport:
          path: /trs/trojan/ws
    trojan-in2:
      meta: # 
        shadowtls_mode: only_me
        tls_s_port: 0 # 如果overshadowtls为true，这一项必填
        support_multiuser: true # 是否支持多用户模式
        server_s_port: 0 # 如果不支持，此项必填，需要为每个用户开一个服务端口，注意不能与其他地方冲突
        uname_key: name
        use_tls: true
        use_transport: true
      content: # 基本模板
        type: trojan
        listen_port: 20234
        transport:
          path: /trs/trojan/ws

# 这里singbox 每种协议出站的一些默认设定
singbox_outbound_default:
  multiplex:
    enabled: true
    protocol: smux
    max_connections: 8
    min_streams: 4

  shadowsocks:
    type: shadowsocks
    network: tcp
    udp_over_tcp: false # 打开后与multiplex冲突
    multiplex:
      enabled: true

# clashmeta 每种协议出站的一些默认设定
clashmeta_proxies_default:
  ss:
    type: ss
    udp: true



